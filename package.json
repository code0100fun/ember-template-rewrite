{
  "name": "ember-template-rewrite",
  "version": "0.0.4",
  "description": "A utility for automatically refactoring Ember.js templates.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/q2ebanking/ember-template-rewrite"
  },
  "scripts": {
    "lint": "eslint lib test bin",
    "babel": "babel . -d dist --ignore node_modules --ignore test --ignore bin --ignore dist",
    "copy": "ncp lib/preprocessor.pegjs dist/lib/preprocessor.pegjs && ncp package.json dist/package.json",
    "build": "npm run babel && npm run copy",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run spec",
    "spec": "./mocha",
    "tdd": "npm run spec -- -w",
    "cover": "nyc --reporter=html ./mocha",
    "coverCI": "nyc npm run cover && nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Chase McCarthy",
  "license": "Apache-2.0",
  "bin": {
    "ember-template-rewrite": "bin/ember-template-rewrite.js"
  },
  "devDependencies": {
    "assert-diff": "^1.1.0",
    "babel-cli": "7.0.0-alpha.7",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "7.0.0-alpha.7",
    "coveralls": "^2.13.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.2.0",
    "ncp": "^2.0.0",
    "nyc": "^10.2.0"
  },
  "dependencies": {
    "babel-runtime": "^7.0.0-alpha.7",
    "broccoli": "^1.1.0",
    "broccoli-funnel": "^1.1.0",
    "broccoli-persistent-filter": "^1.2.13",
    "broccoli-plugin": "^1.3.0",
    "commander": "^2.9.0",
    "deepmerge": "^1.3.1",
    "fs-extra": "^2.0.0",
    "glimmer-engine": "^0.19.3",
    "pegjs": "^0.10.0",
    "underscore": "^1.8.3"
  }
}
